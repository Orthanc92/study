   "metadata": {},
   "outputs": [],
   "source": [
    "input_size = 25 \n",
    "embedding_size = 64  \n",
    "hidden_size = 128  \n",
    "output_size = 25\n",
    "num_epochs = 50000\n",
    "batch_size = 256"
   ]
