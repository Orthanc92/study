   ],
   "source": [
    "results_df = pd.DataFrame({\n",
    "    'Model': ['LSTM_25'],\n",
    "    'Accuracy': [accuracy * 100],\n",
    "    'Correct Predictions': [correct_predictions],\n",
    "    'Total Predictions': [total_predictions]\n",
    "})\n",
    "\n",
    
    "print(results_df)"
